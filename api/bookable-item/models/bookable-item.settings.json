{
  "kind": "collectionType",
  "collectionName": "bookable_items",
  "info": {
    "name": "bookableItem",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "decription": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext",
      "required": true
    },
    "coverPhoto": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "checkfrontId": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "maxLength": 10
    },
    "isActive": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean",
      "default": true
    },
    "gallery": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Duration": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "time"
    },
    "itinerary": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext"
    },
    "highlights": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext"
    },
    "type": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "SHUTTLE",
        "TRANSFER",
        "FLIGHT",
        "TOUR",
        "EVENT",
        "MEDICALSERVICE",
        "BEAUTYTREATMENT"
      ]
    },
    "prices": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "general.shuttle-price",
        "general.transfer-prices",
        "general.adult-child-price"
      ]
    },
    "itemTimes": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "dynamiczone",
      "components": [
        "general.times-repeatable",
        "general.times-date-based"
      ]
    }
  }
}
